@{
    ViewData["Title"] = "Home Page";
}
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">

<div class="d-flex">
    <p>Region:</p>

    <select class="form-select" aria-label="Default select example" style="width: 150px; margin-left: 20px;">
        <option value="poland">Poland</option>
        <option value="saudiarabia">Saudi Arabia</option>
        <option value="usa">USA</option>
    </select>

    <p style="margin-left: 20px;">Errors:</p>

    <input type="range" class="form-range" id="customRange1" min="0" max="1000" style="width: 200px; margin-left: 20px;" onchange="updateTextInput(this.value);">

    <input type="text" id="textInput" class="form-control" style="width: 80px; margin-left: 20px;" />

    <p style="margin-left: 20px;">Seed:</p>
    <input type="text" id="seed" class="form-control" style="width: 100px; margin-left: 20px;" />

    <button class="btn btn-success" style="margin-left: 20px; width: 60px;" onclick="my_seed();"><i class="bi bi-shuffle"></i></button>

    <button type="submit" class="btn btn-primary" style="margin-left: 20px; width: 150px;">Export</button>
</div>
<br />

<script>
    function updateTextInput(val) {
        document.getElementById('textInput').value = val;
    }

    let randomNumber = Math.random();
    function getRandomInt(min, max) {
        min = Math.ceil(min);
        max = Math.floor(max);
        return Math.floor(Math.random() * (max - min)) + min;
    }

    let min = 1000000;
    let max = 9999999;
    let randomInt = getRandomInt(min, max);

    function my_seed(){
        document.getElementById('seed').value = getRandomInt(min, max);
    }
</script>

<table class="table">
    <thead>
    <th>
        Number
    </th>
    <th>
        Id
    </th>
    <th>
        Name
    </th>
    <th>

    </th>
    </thead>
</table>